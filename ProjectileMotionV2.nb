   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"rotToGrav", "[", 
      RowBox[{"omega_", ",", "r_"}], "]"}], " ", "=", " ", 
     RowBox[{"r", "*", 
      RowBox[{"(", 
       RowBox[{"omega", "^", "2"}], ")"}]}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"cartesianToPolar", "[", 
