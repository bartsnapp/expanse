    RowBox[{
     RowBox[{"arcLengthProjectileMotion", "[", 
      RowBox[{
      "t_", ",", "v0_", ",", "\[CapitalTheta]Throw_", ",", 
       "\[CapitalTheta]init_", ",", "r0_", ",", "omega_", ",", "R_"}], "]"}], 
     " ", "=", " ", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Define", " ", "Local", " ", "variables"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
